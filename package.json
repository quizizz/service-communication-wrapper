{
  "name": "@quizizz/service-communication",
  "version": "1.0.0",
  "description": "A communication wrapper to support any interservice communication",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest --coverageReporters='text-summary'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "JEST_JUNIT_OUTPUT_DIR=tests/results/ jest ./tests/unit --detectOpenHandles",
    "test:unit:watch": "JEST_JUNIT_OUTPUT_DIR=tests/results/ jest ./tests/unit --watch --detectOpenHandles",
    "test:unit:ci": "JEST_JUNIT_OUTPUT_DIR=tests/results/ jest --ci --detectOpenHandles --collect-coverage --coverage --json --outputFile=tests/reports/test-report.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quizizz/service-communication-wrapper.git"
  },
  "author": "Quizizz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quizizz/service-communication-wrapper/issues"
  },
  "homepage": "https://github.com/quizizz/service-communication-wrapper#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "opossum": "^8.1.3",
    "prom-client": "^15.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/opossum": "^8.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "opossum-prometheus": "^0.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
